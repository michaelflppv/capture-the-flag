<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="340.0" prefWidth="270.0" styleClass="server-list" stylesheets="@Default.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.sep.cfp4.application.controller.gameSetup.SelectedPiecesListController">
  <children>
    <Label text="Pieces List" />
    <TableView fx:id="pieceTable" prefHeight="331.0" prefWidth="283.0">
      <columns>
        <TableColumn fx:id="nameColumn" maxWidth="60.0" minWidth="60.0" prefWidth="60.0" text="Name" />
        <TableColumn fx:id="viewColumn" maxWidth="60.0" minWidth="60.0" prefWidth="60.0" text="View" />
        <TableColumn fx:id="amountColumn" maxWidth="86.0" minWidth="60.0" prefWidth="74.0" text="Amount" />
        <TableColumn fx:id="deleteColumn" maxWidth="55.0" minWidth="25.0" prefWidth="38.0" />
      </columns>
      <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
    </TableView>
  </children>
</VBox>
