<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<ScrollPane stylesheets="@Default.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.sep.cfp4.application.controller.gamePlay.JoinGameController">
   <content>
      <AnchorPane prefHeight="600.0" prefWidth="600.0">
         <children>
            <Label layoutX="22.0" layoutY="527.0" text="Enter Game Session Token">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <TextField fx:id="gameSessionField" layoutX="22.0" layoutY="552.0" prefHeight="40.0" prefWidth="484.0" promptText="Game Session Token">
               <font>
                  <Font size="16.0" />
               </font>
            </TextField>
            <Button layoutX="511.0" layoutY="552.0" mnemonicParsing="false" onAction="#joinGame" prefHeight="40.0" prefWidth="57.0" styleClass="black-button" text="Join">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <fx:include fx:id="serverList" layoutX="22.0" layoutY="65.0" prefHeight="440.0" prefWidth="550.0" source="ServerList.fxml" />
            <TextField fx:id="teamID" layoutX="22.0" layoutY="483.0" prefHeight="36.0" prefWidth="484.0" promptText="Team Name" />
            <Label layoutX="22.0" layoutY="458.0" text="Team Name" />
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
